{
  "name": "Unprint Me",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Bring private browsing to the server.

  Masks your user agent string and other identifying information from your browser, preventing you from being tracked across the web.",
  "homepage_url": "https://github.com/lidatong/unprintme",
  "icons": {
    "16": "icons/fp16.png",
    "48": "icons/fp48.png",
    "128": "icons/fp128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/fp19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "contextMenus",
    "cookies",
    "notifications",
	"geolocation",
	"webRequest", "webRequestBlocking", "<all_urls>"
  ],
  "web_accessible_resources": [
	"src/inject/injected.js"
  ], "omnibox": {
    "keyword": "extensionizr"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
